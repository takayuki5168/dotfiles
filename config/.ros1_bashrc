# source

function autoware () {
  noetic
  source ~/colcon_ws/missing_noetic_pkgs/install/setup.bash
  source ~/colcon_ws/autoware.proj/install/setup.bash
}
function autoware_jpntaxi () {
  noetic
  source ~/colcon_ws/missing_noetic_pkgs/install/setup.bash
  source ~/colcon_ws/autoware.proj.jpntaxi/install/setup.bash
}
